set nohud
set nosmoothscroll
set noautofocus

set typelinkhints
set numerichints    "requried for typelinkhints

let searchlimit = 20
let scrollstep = 60
let barposition = "bottom"

let mapleader = ","
map d closeTab
map u lastClosedTab

map h previousTab
map l nextTab

map s :tabnew google

map <leader>e :open! chrome://extensions<CR>
map <leader>p :togglepin<CR>

map R reloadTabUncached

map <C-u> scrollPageUp
map <C-d> scrollPageDown

unmap a

map p P     "openPasteTab

let configpath = '/c:/users/alok/.cvimrc'
set localconfig " Update settings via local file.

